{
    "manifest_version": 3,
    "name": "FlexSome",
    "version": "1.0",
    "description": "FlexSome: A simple solution to add up averages and weightages for you.",
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  